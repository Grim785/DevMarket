{
  "name": "devmarket-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "db:migrate": "sequelize-cli db:migrate --config src/config/database.js --migrations-path src/migrations",
    "db:seed": "sequelize-cli db:seed:all --config src/config/database.js --seeders-path src/seeders",
    "db:undo": "sequelize-cli db:migrate:undo:all --config src/config/database.js --migrations-path src/migrations"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.7",
    "sequelize": "^6.37.3",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "stripe": "^19.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2"
  }
}
